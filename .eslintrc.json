{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "no-param-reassign": 0,
        "no-console": 0,
        "no-underscore-dangle": "off",
        "quotes": ["error", "single"]
    }
}
